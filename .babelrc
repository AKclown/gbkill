{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "node 14.0",
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ],
    ["@babel/preset-react", { "runtime": "automatic" }]
  ],
  "plugins": [
    "@babel/plugin-transform-typescript",
    [
      "@babel/plugin-syntax-import-attributes",
      { "deprecatedAssertSyntax": true }
    ]
  ]
}
